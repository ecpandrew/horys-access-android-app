<androidx.cardview.widget.CardView
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:card_view="http://schemas.android.com/apk/res-auto"
        xmlns:app="http://schemas.android.com/tools"
        android:id="@+id/child_card"
        android:layout_width="match_parent"
        android:layout_height="180dp"
        android:minHeight="200dp"
        android:layout_marginTop="8dp"
        android:layout_marginStart="2dp"
        android:layout_marginEnd="32dp"
        card_view:cardElevation="2dp"
        card_view:cardCornerRadius="5dp"
        app:strokeColor="@color/colorPrimary"
        app:strokeWidth="1dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        >
    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="LSDi Laboratory"
                android:textColor="@color/colorPrimary"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" android:id="@+id/child_title"
                android:layout_marginStart="13dp" android:layout_marginTop="8dp"/>
        <ImageButton
                android:layout_width="24dp"
                android:layout_height="wrap_content"
                android:background="@color/transparent"
                android:src="@drawable/ic_dotsdp"
                app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="8dp"
                app:layout_constraintTop_toTopOf="parent" android:layout_marginEnd="8dp"
                android:id="@+id/child_options"/>
        <TableLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:stretchColumns="0,1"
                android:id="@+id/child_table_layout"
                app:layout_constraintTop_toBottomOf="@+id/child_options"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="8dp"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp" android:layout_marginTop="8dp"
                app:layout_constraintHorizontal_bias="0.0">


            <TableRow
                    android:padding="5dp">
                <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:textColor="@color/colorPrimary"
                        android:text="Person"/>

                <RelativeLayout
                        android:id="@+id/child_relative_layout"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="left">

                    <ImageButton
                            android:id="@+id/child_sort_duration"
                            android:layout_width="19dp"
                            android:layout_height="19dp"
                            android:src="@drawable/ic_keyboard_arrow_up_black_24dp"
                            android:background="@color/transparent"
                            android:layout_marginEnd="17dp"
                            android:layout_toLeftOf="@+id/child_text2"
                            android:layout_marginRight="21dp"/>


                    <TextView
                            android:id="@+id/child_text2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:textColor="@color/colorPrimary"
                            android:text="Duration (h)"/>
                </RelativeLayout>
            </TableRow>
            <View android:layout_width="fill_parent" android:layout_height="1px"
                  android:background="@color/grey"
            />

            <TableRow
                    android:layout_height="1dp"
                    android:background="@color/grey">

            </TableRow>

            <TableRow
                    android:padding="5dp">
                <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:textColor="@color/colorPrimary"
                        android:text="AndrÃ© Luiz"/>
                <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:layout_marginLeft="33dp"
                        android:textColor="@color/colorPrimary"
                        android:text="2"/>
            </TableRow>
            <View android:layout_width="fill_parent" android:layout_height="1px"
                  android:background="@color/grey"
            />
            <TableRow
                    android:padding="5dp">
                <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:textColor="@color/colorPrimary"
                        android:text="Alysson"/>
                <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/colorPrimary"
                        android:layout_weight="1"
                        android:layout_marginLeft="33dp"
                        android:text="3"/>
            </TableRow>
            <View android:layout_width="fill_parent" android:layout_height="1px"
                  android:background="@color/grey"
            />
            <View android:layout_width="fill_parent" android:layout_height="1px"
                  android:background="@color/grey"
            />
            <View android:layout_width="fill_parent" android:layout_height="1px"
                  android:background="@color/grey"
            />
        </TableLayout>
        <TextView
                android:text="1-2 of 2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/child_pagination_txt"
                android:textColor="@color/black"
                app:layout_constraintEnd_toStartOf="@+id/child_pagination_left"
                android:layout_marginEnd="16dp"
                android:layout_marginBottom="8dp"
                app:layout_constraintBottom_toBottomOf="parent"/>
        <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@color/transparent"
                app:srcCompat="@drawable/ic_keyboard_arrow_left_black_24dp"
                android:id="@+id/child_pagination_left"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginEnd="8dp"
                app:layout_constraintEnd_toStartOf="@+id/child_pagination_right"
                android:layout_marginBottom="8dp"/>
        <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/ic_keyboard_arrow_right_black_24dp"
                android:background="@color/transparent"
                android:id="@+id/child_pagination_right"
                app:layout_constraintEnd_toEndOf="@+id/child_table_layout"
                android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginEnd="8dp"/>
        <TextView
                android:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/child_footer"
                android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="@+id/child_table_layout"
                android:layout_marginStart="8dp"/>
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.cardview.widget.CardView>