<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:card_view="http://schemas.android.com/tools"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    <com.google.android.material.appbar.AppBarLayout
            android:id="@+id/appBarLayout2"
            android:layout_width="match_parent"
            android:layout_height="56dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent">

        <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbar"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

        </androidx.appcompat.widget.Toolbar>

    </com.google.android.material.appbar.AppBarLayout>


    <androidx.cardview.widget.CardView
            android:id="@+id/card"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:minHeight="200dp" android:layout_marginTop="8dp"
            card_view:cardElevation="2dp"
            card_view:cardCornerRadius="5dp"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="8dp"
            app:strokeColor="@color/colorPrimary"
            app:strokeWidth="1dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/appBarLayout2"
            app:layout_constraintBottom_toTopOf="@+id/tableLayout1">
        <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="UFMA"
                    android:textColor="@color/colorPrimary"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" android:id="@+id/textView7"
                    android:layout_marginStart="13dp" android:layout_marginTop="8dp"/>
            <ImageButton
                    android:layout_width="24dp"
                    android:layout_height="wrap_content"
                    android:background="@color/transparent"
                    android:src="@drawable/ic_dotsdp" app:layout_constraintBottom_toTopOf="@+id/tableLayout2"
                    app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="8dp"
                    app:layout_constraintTop_toTopOf="parent" android:layout_marginEnd="8dp"
                    android:layout_marginBottom="8dp"/>
            <TableLayout
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:stretchColumns="0,1"
                    android:id="@+id/tableLayout2"
                    app:layout_constraintTop_toBottomOf="@+id/textView7" app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginStart="8dp" android:layout_marginBottom="8dp"
                    app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginEnd="8dp" android:layout_marginTop="8dp">


                <TableRow
                        android:padding="5dp">
                    <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:textColor="@color/colorPrimary"
                            android:text="Person"/>

                    <RelativeLayout
                            android:id="@+id/relativeLayout2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="left">

                        <ImageButton
                                android:id="@+id/imageButton1"
                                android:layout_width="19dp"
                                android:layout_height="19dp"
                                android:src="@drawable/ic_keyboard_arrow_up_black_24dp"
                                android:background="@color/transparent"
                                android:layout_marginEnd="20dp"
                                android:layout_toLeftOf="@+id/text2"
                                android:layout_marginRight="21dp"/>


                        <TextView
                                android:id="@+id/text2"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:textColor="@color/colorPrimary"
                                android:text="Total Duration (h)"/>
                    </RelativeLayout>
                </TableRow>
                <View android:layout_width="fill_parent" android:layout_height="1px"
                      android:background="@color/grey"
                />

                <TableRow
                        android:layout_height="1dp"
                        android:background="@color/grey">

                </TableRow>

                <TableRow
                        android:padding="5dp">
                    <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:textColor="@color/colorPrimary"
                            android:text="André Luiz"/>
                    <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:layout_marginLeft="33dp"
                            android:textColor="@color/colorPrimary"
                            android:text="2"/>
                </TableRow>
                <View android:layout_width="fill_parent" android:layout_height="1px"
                      android:background="@color/grey"
                />
                <TableRow
                        android:padding="5dp">
                    <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:textColor="@color/colorPrimary"
                            android:text="Alysson"/>
                    <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="@color/colorPrimary"
                            android:layout_weight="1"
                            android:layout_marginLeft="33dp"
                            android:text="3"/>
                </TableRow>
                <View android:layout_width="fill_parent" android:layout_height="1px"
                      android:background="@color/grey"
                />
                <TableRow
                        android:padding="5dp">
                    <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:textColor="@color/colorPrimary"
                            android:text="Daniel CP"/>
                    <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:layout_marginLeft="33dp"
                            android:textColor="@color/colorPrimary"
                            android:text="0.1"/>
                </TableRow>
                <View android:layout_width="fill_parent" android:layout_height="1px"
                      android:background="@color/grey"
                />
                <TableRow
                        android:padding="5dp">
                    <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:textColor="@color/colorPrimary"
                            android:text="Francisco Silva"/>
                    <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:layout_marginLeft="33dp"
                            android:textColor="@color/colorPrimary"
                            android:text="1"/>
                </TableRow>
                <View android:layout_width="fill_parent" android:layout_height="1px"
                      android:background="@color/grey"
                />
            </TableLayout>
            <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:textColor="@color/grey"
                    android:text="Footer"
                    android:id="@+id/textView6" card_view:layout_editor_absoluteY="226dp"
                    card_view:layout_editor_absoluteX="8dp"/>
            <TextView
                    android:text="1-5 of 10"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView5"
                    app:layout_constraintEnd_toStartOf="@+id/imageButton"
                    android:layout_marginEnd="16dp" android:layout_marginTop="10dp"
                    app:layout_constraintTop_toBottomOf="@+id/tableLayout2"
                    app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="11dp"/>
            <ImageButton
                    android:layout_width="wrap_content"
                    android:layout_height="24dp"
                    android:background="@color/transparent"
                    app:srcCompat="@drawable/ic_keyboard_arrow_left_black_24dp"
                    android:id="@+id/imageButton"
                    card_view:ignore="VectorDrawableCompat"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:layout_marginBottom="8dp"
                    android:layout_marginEnd="8dp" app:layout_constraintEnd_toStartOf="@+id/imageButton2"
                    android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/tableLayout2"/>
            <ImageButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:srcCompat="@drawable/ic_keyboard_arrow_right_black_24dp"
                    android:background="@color/transparent"
                    android:id="@+id/imageButton2"
                    card_view:ignore="VectorDrawableCompat"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:layout_marginTop="3dp" app:layout_constraintTop_toBottomOf="@+id/tableLayout2"
                    app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
                    android:layout_marginBottom="8dp" app:layout_constraintVertical_bias="1.0"/>
        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>


    <TableLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:stretchColumns="*"
            android:id="@+id/tableLayout1"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp">


        <TableRow
                android:padding="5dp">

            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:textColor="@color/colorPrimary"
                    android:textStyle="bold"
                    android:text="LSDi Laboratory"/>
            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:textColor="@color/colorPrimary"
                    android:textStyle="bold"
                    android:text="Duration (h)"/>

        </TableRow>

        <TableRow
                android:background="#ECEFF1"
                android:padding="5dp">

            <TextView
                    android:layout_width="wrap_content"
                    android:textColor="@color/colorPrimary"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="André Luiz"/>
            <TextView
                    android:layout_width="wrap_content"
                    android:textColor="@color/colorPrimary"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="2"/>
        </TableRow>
        <TableRow
                android:background="#ECEFF1"
                android:padding="5dp">

            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/colorPrimary"
                    android:layout_weight="1"
                    android:text="Alysson"/>
            <TextView
                    android:layout_width="wrap_content"
                    android:textColor="@color/colorPrimary"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="3"/>
        </TableRow>


        <TableRow
                android:background="@color/grey"
                android:padding="5dp">

            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:textColor="@color/colorPrimary"
                    android:textStyle="bold"
                    android:text="Server Room"/>
            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:textColor="@color/colorPrimary"
                    android:textStyle="bold"
                    android:text="Duration (h)"/>

        </TableRow>


        <TableRow
                android:background="#ECEFF1"
                android:padding="5dp">

            <TextView
                    android:layout_width="wrap_content"
                    android:textColor="@color/colorPrimary"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="Daniel CP"/>
            <TextView
                    android:layout_width="wrap_content"
                    android:textColor="@color/colorPrimary"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="0.1"/>
        </TableRow>


        <TableRow
                android:background="@color/grey"
                android:padding="5dp">

            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:textColor="@color/colorPrimary"
                    android:textStyle="bold"
                    android:text="Reunion Room"/>
            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:textColor="@color/colorPrimary"
                    android:textStyle="bold"
                    android:text="Duration (h)"/>

        </TableRow>


        <TableRow
                android:background="#ECEFF1"
                android:padding="5dp">

            <TextView
                    android:layout_width="wrap_content"
                    android:textColor="@color/colorPrimary"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="Francisco Silva"/>
            <TextView
                    android:layout_width="wrap_content"
                    android:textColor="@color/colorPrimary"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="1"/>
        </TableRow>


    </TableLayout>


</androidx.constraintlayout.widget.ConstraintLayout>