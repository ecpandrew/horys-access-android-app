<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:clickable="true"
        android:focusable="true"
        android:foreground="?android:attr/selectableItemBackground"
        android:orientation="vertical" tools:targetApi="m" tools:ignore="UnusedAttribute">


    <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/selectionHistoryRecyclerView"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toBottomOf="@+id/date_card_view"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/backToParent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp" android:layout_marginTop="8dp"/>
    <Button
            android:text="Back to Parent"
            android:layout_width="202dp"
            android:layout_height="wrap_content"
            android:id="@+id/backToParent"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toStartOf="@+id/buttonGet"/>
    <Button
            android:text="GET"
            android:layout_width="202dp"
            android:layout_height="wrap_content"
            android:id="@+id/buttonGet"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toEndOf="@+id/backToParent"
            app:layout_constraintHorizontal_bias="0.5"/>

    <androidx.cardview.widget.CardView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/date_card_view"

            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
            <TextView
                    android:text="Data Inicial"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="8sp"
                    android:textColor="@color/colorPrimary"
                    android:id="@+id/textView15" app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginStart="3dp" android:layout_marginTop="3dp"
                    app:layout_constraintTop_toTopOf="parent"/>
            <TextView
                    android:id="@+id/dayTV"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="07"
                    android:paddingTop="0dp"
                    android:paddingLeft="0dp"
                    android:paddingBottom="0dp"
                    android:paddingRight="0dp"
                    android:textColor="@color/colorPrimary"
                    android:textSize="15sp"
                    android:layout_marginStart="3dp"
                    app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginTop="3dp" app:layout_constraintTop_toBottomOf="@+id/textView15"/>
            <TextView
                    android:text="JUN"
                    android:layout_width="wrap_content"
                    android:textColor="@color/colorPrimary"
                    android:layout_height="wrap_content"
                    android:textSize="8sp"
                    android:id="@+id/monthTV" app:layout_constraintStart_toEndOf="@+id/dayTV"
                    android:layout_marginStart="3dp" android:layout_marginTop="3dp"
                    app:layout_constraintTop_toBottomOf="@+id/textView15"
                    android:textAllCaps="true"/>
            <TextView
                    android:text="2019"
                    android:layout_width="wrap_content"
                    android:textColor="@color/colorPrimary"
                    android:layout_height="wrap_content"

                    android:id="@+id/yearTV" app:layout_constraintStart_toEndOf="@+id/dayTV"
                    android:layout_marginStart="3dp"
                    app:layout_constraintTop_toBottomOf="@+id/monthTV" android:layout_marginTop="3dp"
                    android:textSize="8sp"
            />
            <TextView
                    android:text="16:42"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/colorPrimary"
                    android:id="@+id/timeTV"
                    android:textSize="8sp"

                    app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginStart="3dp"
                    app:layout_constraintTop_toBottomOf="@+id/view"
                    android:layout_marginTop="3dp" android:paddingBottom="0dp"/>
            <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="27dp" app:srcCompat="@drawable/ic_calendar"
                    android:id="@+id/imageView" app:layout_constraintStart_toEndOf="@+id/yearTV"
                    android:layout_marginStart="3dp" app:layout_constraintBottom_toTopOf="@+id/view"
                    android:layout_marginBottom="5dp"
            />
            <View
                    android:layout_width="0dp"
                    android:layout_height="2dp"
                    android:background="@color/colorPrimary"
                    app:layout_constraintTop_toBottomOf="@+id/yearTV"
                    android:id="@+id/view" android:layout_marginTop="3dp"
                    app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"/>

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>


    <androidx.cardview.widget.CardView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/date_card_view2"

            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
            <TextView
                    android:text="Data Final"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="8sp"
                    android:textColor="@color/colorPrimary"
                    android:id="@+id/textView2"
                    android:layout_marginEnd="3dp"
                    app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="3dp"
                    app:layout_constraintTop_toTopOf="parent"/>
            <TextView
                    android:id="@+id/dayTV2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="07"
                    android:paddingTop="0dp"
                    android:paddingLeft="0dp"
                    android:paddingBottom="0dp"
                    android:paddingRight="0dp"
                    android:textColor="@color/colorPrimary"
                    android:textSize="15sp"
                    android:layout_marginEnd="3dp" app:layout_constraintEnd_toStartOf="@+id/yearTV2"
                    android:layout_marginTop="3dp" app:layout_constraintTop_toBottomOf="@+id/textView2"/>
            <TextView
                    android:text="jun"
                    android:layout_width="wrap_content"
                    android:textColor="@color/colorPrimary"
                    android:layout_height="wrap_content"
                    android:textSize="8sp"
                    android:id="@+id/monthTV2"
                    android:layout_marginEnd="3dp"
                    android:textAllCaps="true"
                    app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="3dp"
                    app:layout_constraintTop_toBottomOf="@+id/textView2"/>
            <TextView
                    android:text="2019"
                    android:layout_width="wrap_content"
                    android:textColor="@color/colorPrimary"
                    android:layout_height="wrap_content"
                    android:id="@+id/yearTV2"
                    android:textSize="8sp"
                    android:layout_marginEnd="3dp"
                    app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="3dp"
                    app:layout_constraintTop_toBottomOf="@+id/monthTV2"
            />
            <TextView
                    android:text="18:00"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/colorPrimary"
                    android:id="@+id/timeTV2"
                    android:textSize="8sp"

                    app:layout_constraintTop_toBottomOf="@+id/view2"
                    android:layout_marginTop="3dp" android:paddingBottom="0dp" android:layout_marginEnd="3dp"
                    app:layout_constraintEnd_toEndOf="parent"/>
            <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="27dp" app:srcCompat="@drawable/ic_calendar"
                    android:id="@+id/imageView2"
                    app:layout_constraintBottom_toTopOf="@+id/view2"
                    android:layout_marginBottom="5dp"
                    app:layout_constraintEnd_toStartOf="@+id/dayTV2" android:layout_marginEnd="3dp"/>
            <View
                    android:layout_width="0dp"
                    android:layout_height="2dp"
                    android:background="@color/colorPrimary"
                    app:layout_constraintTop_toBottomOf="@+id/yearTV2"
                    android:id="@+id/view2" android:layout_marginTop="3dp"
                    app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"/>

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>


</androidx.constraintlayout.widget.ConstraintLayout>