                        android:padding="5dp">
                    <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:textColor="@color/colorPrimary"
                            android:text="Nome"/>

                    <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:textColor="@color/colorPrimary"
                            android:text="Location" android:id="@+id/textView52"/>
                    <RelativeLayout
                            android:id="@+id/parent_relative_layout2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="left">

                        <ImageButton
                                android:id="@+id/parent_sort_duration2"
                                android:layout_width="19dp"
                                android:layout_height="19dp"
                                android:src="@drawable/ic_keyboard_arrow_up_black_24dp"
                                android:background="@color/transparent"
                                android:layout_marginEnd="5dp"
                                android:layout_toLeftOf="@+id/parent_text22"
                                android:layout_marginRight="21dp"/>


                        <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:textColor="@color/colorPrimary"
                                android:text="Duration (h)" android:id="@+id/parent_text22"/>
                    </RelativeLayout>
                </TableRow>
                <View android:layout_width="fill_parent" android:layout_height="1px"
                      android:background="@color/grey"
                />

                <TableRow
                        android:padding="5dp" android:layout_width="match_parent"
                        android:layout_height="match_parent">
                    <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:textColor="@color/colorPrimary"
                            android:text="Aluno M. 1"/>
                    <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:textColor="@color/colorPrimary"
                            android:text="LSDi Lab" android:id="@+id/textView62"/>
                    <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:layout_marginLeft="33dp"
                            android:textColor="@color/colorPrimary"
                            android:text="3"/>
                </TableRow>
                <View android:layout_width="fill_parent" android:layout_height="1px"
                      android:background="@color/grey"
                />
                <TableRow
                        android:padding="5dp" android:layout_width="match_parent"
                        android:layout_height="match_parent">
                    <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:textColor="@color/colorPrimary"
                            android:text="Aluno M. 2"/>
                    <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:textColor="@color/colorPrimary"
                            android:text="Server Room" android:id="@+id/textView72"/>
                    <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:layout_marginLeft="33dp"
                            android:textColor="@color/colorPrimary"
                            android:text="0.2"/>
                </TableRow>
                <View android:layout_width="fill_parent" android:layout_height="1px"
                      android:background="@color/grey"
                />
                <TableRow
                        android:padding="5dp">
                    <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:textColor="@color/colorPrimary"
                            android:text="Aluno M. 3"/>
                    <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:textColor="@color/colorPrimary"
                            android:text="Reunion Room" android:id="@+id/textView82"/>
                    <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:layout_marginLeft="33dp"
                            android:textColor="@color/colorPrimary"
                            android:text="2"/>
                </TableRow>
                <View android:layout_width="fill_parent" android:layout_height="1px"
                      android:background="@color/grey"
                />
            </TableLayout>
            <TextView
                    android:text="1-3 of 6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/parent_pagination_txt2"
                    android:textColor="@color/black"
                    app:layout_constraintEnd_toStartOf="@+id/parent_pagination_left2"
                    android:layout_marginEnd="16dp"
                    android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
                    android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/parent_table_layout2"/>
            <ImageButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@color/transparent"
